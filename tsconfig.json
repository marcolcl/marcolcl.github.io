{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es5", "es6", "es7", "es2017", "dom"],
    "jsx": "preserve",
    "strict": true,
    "noEmit": true,
    "sourceMap": true,
    "allowJs": false,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true
  },
  "exclude": ["node_modules", "public", ".cache"]
}
